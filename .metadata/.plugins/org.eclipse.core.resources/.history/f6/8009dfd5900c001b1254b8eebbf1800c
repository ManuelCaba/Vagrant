<?php

    namespace class;
    
    class Persona
    {
        private int $edad;
        
        public function getEdad()
        {
            return $edad;
        }
        
        public function setEdad($edad)
        {
            $this->edad = $edad;
        }
        
        public function getAnhoNacimiento()
        {
            return date("Y") - $edad;
        }
}